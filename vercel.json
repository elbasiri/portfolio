{
     "builds": [
       {
         "src": "composer.json",
         "use": "@vercel/php"
       },
       {
         "src": "public/**/*",
         "use": "@vercel/static"
       }
     ],
     "routes": [
       {
         "src": "/(.*)",
         "dest": "/public/$1",
         "headers": {
           "cache-control": "s-maxage=1, stale-while-revalidate"
         }
       }
     ]
   }
   